{
    "background": {
        "page": "html/background.html"
    }, 
    "browser_action": {
        "default_icon": "img/eye.png", 
        "default_popup": "html/popup.html", 
        "default_title": "Eyebrowse"
    }, 
    "content_scripts": [
        {   
            "css": [
                "css/values.css"
            ], 
            "js": [
                "libs/jquery-1.8.2.min.js", 
                "libs/mustache.min.js", 
                "libs/underscore.min.js", 
                "libs/jquery.idle.js", 
                "js/common.js", 
                "js/prompt.js", 
                "js/idle-detection.js", 
                "js/values.js"
            ], 
            "matches": [
                "<all_urls>"
            ]
        }
    ], 
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'", 
    "description": "Eyebrowse by MIT CSAIL Haystack Group", 
    "icons": {
        "128": "img/eye-128.png", 
        "16": "img/eye-16.png", 
        "48": "img/eye-48.png"
    }, 
    "manifest_version": 2, 
    "name": "Eyebrowse", 
    "permissions": [
        "*://*/*", 
        "http://eyebrowse-staging.csail.mit.edu/*", 
        "http://eyebrowse.csail.mit.edu/*", 
        "https://eyebrowse.csail.mit.edu/*", 
        "http://localhost:8000/*", 
        "cookies", 
        "tabs", 
        "webNavigation"
    ], 
    "version": "0.0.118", 
    "web_accessible_resources": [
        "css/prompt.css", 
        "html/prompt.html", 
        "img/pano-logo.png"
    ]
}