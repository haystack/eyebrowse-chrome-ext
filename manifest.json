{
    "manifest_version": 2,
    "name" : "Eyebrowse",
    "version" : "1.0",
    "description" : "Eyebrowse by CSAIL",
    "browser_action" : {
        "default_icon" : "img/eye.png",
        "default_popup" : "popup.html",
        "default_title" : "Eyebrowse"
    },
    "background" : {
            "page": "background.html"
        },
        "permissions" : [
            "notifications",
            "tabs",
            "background",
            "storage"
        ]
}
